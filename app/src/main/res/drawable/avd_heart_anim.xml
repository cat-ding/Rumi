<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="1000dp"
            android:height="1000dp"
            android:viewportWidth="1000"
            android:viewportHeight="1000">
            <group
                android:name="group"
                android:pivotX="500"
                android:pivotY="500">
                <path
                    android:name="path"
                    android:pathData="M 500 952.3 L 428.9 888.2 C 176.6 656.5 10 506.1 10 318.8 C 10 166 127.6 47.7 279.5 47.7 C 365.2 47.7 446.1 87.1 500 151.2 C 553.9 87.1 634.7 47.7 720.5 47.7 C 872.4 47.7 990 166 990 318.8 C 990 506.1 823.4 656.5 571.1 888.2 L 500 952.3 Z"
                    android:fillColor="@color/colorPrimary"
                    android:strokeWidth="1"/>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:duration="400"
                    android:valueFrom="0"
                    android:valueTo="0.85"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:duration="400"
                    android:valueFrom="0"
                    android:valueTo="0.85"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:startOffset="600"
                    android:duration="200"
                    android:valueFrom="0.85"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:startOffset="600"
                    android:duration="200"
                    android:valueFrom="0.85"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="fillAlpha"
                    android:duration="50"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="fillAlpha"
                    android:startOffset="750"
                    android:duration="50"
                    android:valueFrom="1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>
